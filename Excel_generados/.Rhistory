train_test <- function(df, size = 0.8, train = TRUE) {
n_filas = nrow(df)
total_filas = size * n_filas
train_sample <- 1: total_filas
if (train == TRUE) {
return (df[train_sample, ])
} else {
return (df[-train_sample, ])
}
}
